<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:layout_width="match_parent" android:layout_height="@dimen/search_dialog_searen_engine_item"
        android:paddingLeft="6dip" android:paddingRight="6dip">
	<TextView android:id="@+id/search_eninge_label" android:layout_width="wrap_content"
		android:layout_height="match_parent" android:layout_alignParentLeft="true" 
                android:layout_alignParentTop="true" android:layout_alignParentBottom="true"
                android:layout_toLeftOf="@+id/search_eninge_favicon" android:textSize="16sp"
                android:singleLine="true" android:ellipsize="end"  android:paddingRight="6dip" 
                android:gravity="center_vertical" android:minHeight="@dimen/search_dialog_searen_engine_item"
                android:textStyle="normal" android:textColor="@android:color/primary_text_light" />
        <ImageView android:id="@id/search_eninge_favicon" android:layout_width="20dip"
		android:layout_height="20dip"  android:layout_alignParentTop="true"
                android:layout_alignParentBottom="true" android:layout_alignParentRight="true" 
                android:scaleType="fitXY" android:layout_centerVertical="true" />
</RelativeLayout>
