<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent">
    
	<Button android:text="@string/label_save"
	    android:layout_width="fill_parent"
	    android:layout_height="wrap_content"
		 android:layout_margin="5dip"
		 android:id="@+id/save_btn"/>    
		
	<ScrollView android:layout_width="fill_parent"
	    android:layout_height="fill_parent">
	<LinearLayout
	    android:orientation="vertical"
	    android:layout_width="fill_parent"
	    android:layout_height="fill_parent">

       	<TextView android:text="@string/provisioning_auto_config_mode"
	    android:paddingTop="5dip"
	    android:layout_width="fill_parent"
	    android:layout_height="wrap_content"/>
		<Spinner android:id="@+id/Autoconfig"
		android:layout_height="wrap_content"
		android:layout_width="fill_parent"
		android:layout_marginLeft="5dip"
		android:layout_marginRight="5dip"
		android:drawSelectorOnTop="true"/>

        <TextView android:text="@string/secondary_provisioning_server"
        android:paddingTop="5dip"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"/>
        <EditText android:id="@+id/SecondaryProvisioningAddress"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="5dip"
        android:layout_marginRight="5dip"
        android:maxLength="100" android:lines="1"/>

 <CheckBox android:id="@+id/SecondaryProvisioningAddressOnly"
		android:layout_height="wrap_content"
		android:layout_width="fill_parent"
		android:layout_marginLeft="5dip"
		android:layout_marginRight="5dip"
        android:text="@string/provisioning_use_only_the_secondary_provisioning_server"/>

       	<TextView android:text="@string/provisioning_network_access"
	    android:paddingTop="5dip"
	    android:layout_width="fill_parent"
	    android:layout_height="wrap_content"/>
		<Spinner android:id="@+id/NetworkAccess"
		android:layout_height="wrap_content"
		android:layout_width="fill_parent"
		android:layout_marginLeft="5dip"
		android:layout_marginRight="5dip"
		android:drawSelectorOnTop="true"/>

       	<TextView android:text="@string/provisioning_IMS_service_polling_period_secs"
	    android:paddingTop="5dip"
	    android:layout_width="fill_parent"
	    android:layout_height="wrap_content"/>
		<EditText android:id="@+id/ImsServicePollingPeriod"
	    android:layout_width="fill_parent"
	    android:layout_height="wrap_content"
	 	android:layout_marginLeft="5dip"
	 	android:layout_marginRight="5dip"
	 	android:maxLength="10" android:inputType="number" android:lines="1"/>

       	<TextView android:text="@string/provisioning_default_protocol_for_file_transfer"
	    android:paddingTop="5dip"
	    android:layout_width="fill_parent"
	    android:layout_height="wrap_content"/>
		<Spinner android:id="@+id/FtProtocol"
		android:layout_height="wrap_content"
		android:layout_width="fill_parent"
		android:layout_marginLeft="5dip"
		android:layout_marginRight="5dip"
		android:drawSelectorOnTop="true"/>
		
		<TextView android:text="@string/provisioning_default_SIP_protocol_for_Mobile_access"
	    android:paddingTop="5dip"
	    android:layout_width="fill_parent"
	    android:layout_height="wrap_content"/>
		<Spinner android:id="@+id/SipDefaultProtocolForMobile"
		android:layout_height="wrap_content"
		android:layout_width="fill_parent"
		android:layout_marginLeft="5dip"
		android:layout_marginRight="5dip"
		android:drawSelectorOnTop="true"/>

	    <TextView android:text="@string/provisioning_default_SIP_protocol_for_Wi_Fi_access"
	    android:paddingTop="5dip"
	    android:layout_width="fill_parent"
	    android:layout_height="wrap_content"/>
		<Spinner android:id="@+id/SipDefaultProtocolForWifi"
		android:layout_height="wrap_content"
		android:layout_width="fill_parent"
		android:layout_marginLeft="5dip"
		android:layout_marginRight="5dip"
		android:drawSelectorOnTop="true"/>

		<TextView android:text="@string/provisioning_TLS_root_or_self_signed_certificate_from__sdcard"
	    android:paddingTop="5dip"
	    android:layout_width="fill_parent"
	    android:layout_height="wrap_content"/>
		<Spinner android:id="@+id/TlsCertificateRoot"
		android:layout_height="wrap_content"
		android:layout_width="fill_parent"
		android:layout_marginLeft="5dip"
		android:layout_marginRight="5dip"
		android:drawSelectorOnTop="true"/>

		<TextView android:text="@string/provisioning_TLS_intermediate_certificate_from_sdcard"
	    android:paddingTop="5dip"
	    android:layout_width="fill_parent"
	    android:layout_height="wrap_content"/>
		<Spinner android:id="@+id/TlsCertificateIntermediate"
		android:layout_height="wrap_content"
		android:layout_width="fill_parent"
		android:layout_marginLeft="5dip"
		android:layout_marginRight="5dip"
		android:drawSelectorOnTop="true"/>

       	<TextView android:text="@string/provisioning_default_SIP_port"
	    android:paddingTop="5dip"
	    android:layout_width="fill_parent"
	    android:layout_height="wrap_content"/>
		<EditText android:id="@+id/SipListeningPort"
	    android:layout_width="fill_parent"
	    android:layout_height="wrap_content"
	 	android:layout_marginLeft="5dip"
	 	android:layout_marginRight="5dip"
	 	android:maxLength="5" android:inputType="number" android:lines="1"/>

       	<TextView android:text="@string/provisioning_SIP_timer_base_T1_milli_secs"
	    android:paddingTop="5dip"
	    android:layout_width="fill_parent"
	    android:layout_height="wrap_content"/>
		<EditText android:id="@+id/SipTimerT1"
	    android:layout_width="fill_parent"
	    android:layout_height="wrap_content"
	 	android:layout_marginLeft="5dip"
	 	android:layout_marginRight="5dip"
	 	android:maxLength="5" android:inputType="number" android:lines="1"/>

        <TextView
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:paddingTop="5dip"
            android:text="@string/provisioning_SIP_timer_T2_milli_secs" />
        <EditText
            android:id="@+id/SipTimerT2"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="5dip"
            android:layout_marginRight="5dip"
            android:inputType="number"
            android:lines="1"
            android:maxLength="5" />

        <TextView
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:paddingTop="5dip"
            android:text="@string/provisioning_SIP_timer_T4_milli_secs" />
        <EditText
            android:id="@+id/SipTimerT4"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="5dip"
            android:layout_marginRight="5dip"
            android:inputType="number"
            android:lines="1"
            android:maxLength="5" />

		<TextView android:text="@string/provisioning_SIP_keep_alive_period_secs"
	    android:paddingTop="5dip"
	    android:layout_width="fill_parent"
	    android:layout_height="wrap_content"/>
		<EditText android:id="@+id/SipKeepAlivePeriod"
	    android:layout_width="fill_parent"
	    android:layout_height="wrap_content"
	 	android:layout_marginLeft="5dip"
	 	android:layout_marginRight="5dip"
	 	android:maxLength="5" android:inputType="number" android:lines="1"/>

       	<TextView android:text="@string/provisioning_SIP_transaction_timeout_secs"
	    android:paddingTop="5dip"
	    android:layout_width="fill_parent"
	    android:layout_height="wrap_content"/>
		<EditText android:id="@+id/SipTransactionTimeout"
	    android:layout_width="fill_parent"
	    android:layout_height="wrap_content"
	 	android:layout_marginLeft="5dip"
	 	android:layout_marginRight="5dip"
	 	android:maxLength="5" android:inputType="number" android:lines="1"/>

       	<TextView android:text="@string/provisioning_default_TCP_port_for_MSRP_session"
	    android:paddingTop="5dip"
	    android:layout_width="fill_parent"
	    android:layout_height="wrap_content"/>
		<EditText android:id="@+id/DefaultMsrpPort"
	    android:layout_width="fill_parent"
	    android:layout_height="wrap_content"
	 	android:layout_marginLeft="5dip"
	 	android:layout_marginRight="5dip"
	 	android:maxLength="5" android:inputType="number" android:lines="1"/>

       	<TextView android:text="@string/provisioning_default_UDP_port_for_RTP_session"
	    android:paddingTop="5dip"
	    android:layout_width="fill_parent"
	    android:layout_height="wrap_content"/>
		<EditText android:id="@+id/DefaultRtpPort"
	    android:layout_width="fill_parent"
	    android:layout_height="wrap_content"
	 	android:layout_marginLeft="5dip"
	 	android:layout_marginRight="5dip"
	 	android:maxLength="5" android:inputType="number" android:lines="1"/>

       	<TextView android:text="@string/provisioning_MSRP_transaction_timeout_secs"
	    android:paddingTop="5dip"
	    android:layout_width="fill_parent"
	    android:layout_height="wrap_content"/>
		<EditText android:id="@+id/MsrpTransactionTimeout"
	    android:layout_width="fill_parent"
	    android:layout_height="wrap_content"
	 	android:layout_marginLeft="5dip"
	 	android:layout_marginRight="5dip"
	 	android:maxLength="5" android:inputType="number" android:lines="1"/>

       	<TextView android:text="@string/provisioning_registration_expire_period_secs"
	    android:paddingTop="5dip"
	    android:layout_width="fill_parent"
	    android:layout_height="wrap_content"/>
		<EditText android:id="@+id/RegisterExpirePeriod"
	    android:layout_width="fill_parent"
	    android:layout_height="wrap_content"
	 	android:layout_marginLeft="5dip"
	 	android:layout_marginRight="5dip"
	 	android:maxLength="10" android:inputType="number" android:lines="1"/>

       	<TextView android:text="@string/provisioning_registration_retry_base_time_secs"
	    android:paddingTop="5dip"
	    android:layout_width="fill_parent"
	    android:layout_height="wrap_content"/>
		<EditText android:id="@+id/RegisterRetryBaseTime"
	    android:layout_width="fill_parent"
	    android:layout_height="wrap_content"
	 	android:layout_marginLeft="5dip"
	 	android:layout_marginRight="5dip"
	 	android:maxLength="10" android:inputType="number" android:lines="1"/>

       	<TextView android:text="@string/provisioning_registration_retry_max_time_secs"
	    android:paddingTop="5dip"
	    android:layout_width="fill_parent"
	    android:layout_height="wrap_content"/>
		<EditText android:id="@+id/RegisterRetryMaxTime"
	    android:layout_width="fill_parent"
	    android:layout_height="wrap_content"
	 	android:layout_marginLeft="5dip"
	 	android:layout_marginRight="5dip"
	 	android:maxLength="10" android:inputType="number" android:lines="1"/>

       	<TextView android:text="@string/provisioning_publish_expire_period_secs"
	    android:paddingTop="5dip"
	    android:layout_width="fill_parent"
	    android:layout_height="wrap_content"/>
		<EditText android:id="@+id/PublishExpirePeriod"
	    android:layout_width="fill_parent"
	    android:layout_height="wrap_content"
	 	android:layout_marginLeft="5dip"
	 	android:layout_marginRight="5dip"
	 	android:maxLength="10" android:inputType="number" android:lines="1"/>

       	<TextView android:text="@string/provisioning_revoke_timeout_secs"
	    android:paddingTop="5dip"
	    android:layout_width="fill_parent"
	    android:layout_height="wrap_content"/>
		<EditText android:id="@+id/RevokeTimeout"
	    android:layout_width="fill_parent"
	    android:layout_height="wrap_content"
	 	android:layout_marginLeft="5dip"
	 	android:layout_marginRight="5dip"
	 	android:maxLength="10" android:inputType="number" android:lines="1"/>

       	<TextView android:text="@string/provisioning_ringing_session_period_secs"
	    android:paddingTop="5dip"
	    android:layout_width="fill_parent"
	    android:layout_height="wrap_content"/>
		<EditText android:id="@+id/RingingPeriod"
	    android:layout_width="fill_parent"
	    android:layout_height="wrap_content"
	 	android:layout_marginLeft="5dip"
	 	android:layout_marginRight="5dip"
	 	android:maxLength="10" android:inputType="number" android:lines="1"/>

       	<TextView android:text="@string/provisioning_subscribe_expiration_timeout_secs"
	    android:paddingTop="5dip"
	    android:layout_width="fill_parent"
	    android:layout_height="wrap_content"/>
		<EditText android:id="@+id/SubscribeExpirePeriod"
	    android:layout_width="fill_parent"
	    android:layout_height="wrap_content"
	 	android:layout_marginLeft="5dip"
	 	android:layout_marginRight="5dip"
	 	android:maxLength="10" android:inputType="number" android:lines="1"/>

       	<TextView android:text="@string/provisioning_Is_composing_timeout_for_chat_secs"
	    android:paddingTop="5dip"
	    android:layout_width="fill_parent"
	    android:layout_height="wrap_content"/>
		<EditText android:id="@+id/IsComposingTimeout"
	    android:layout_width="fill_parent"
	    android:layout_height="wrap_content"
	 	android:layout_marginLeft="5dip"
	 	android:layout_marginRight="5dip"
	 	android:maxLength="5" android:inputType="number" android:lines="1"/>

       	<TextView android:text="@string/provisioning_session_refresh_expire_period_secs"
	    android:paddingTop="5dip"
	    android:layout_width="fill_parent"
	    android:layout_height="wrap_content"/>
		<EditText android:id="@+id/SessionRefreshExpirePeriod"
	    android:layout_width="fill_parent"
	    android:layout_height="wrap_content"
	 	android:layout_marginLeft="5dip"
	 	android:layout_marginRight="5dip"
	 	android:maxLength="10" android:inputType="number" android:lines="1"/>

       	<TextView android:text="@string/provisioning_capability_refresh_timeout_secs"
	    android:paddingTop="5dip"
	    android:layout_width="fill_parent"
	    android:layout_height="wrap_content"/>
		<EditText android:id="@+id/CapabilityRefreshTimeout"
	    android:layout_width="fill_parent"
	    android:layout_height="wrap_content"
	 	android:layout_marginLeft="5dip"
	 	android:layout_marginRight="5dip"
	 	android:maxLength="10" android:lines="1" android:inputType="numberSigned"/>

       	<TextView android:text="@string/provisioning_capability_expiry_timeout_secs"
	    android:paddingTop="5dip"
	    android:layout_width="fill_parent"
	    android:layout_height="wrap_content"/>
		<EditText android:id="@+id/CapabilityExpiryTimeout"
	    android:layout_width="fill_parent"
	    android:layout_height="wrap_content"
	 	android:layout_marginLeft="5dip"
	 	android:layout_marginRight="5dip"
	 	android:maxLength="10" android:lines="1" android:inputType="numberSigned"/>

       	<TextView android:text="@string/provisioning_capability_polling_period_secs"
	    android:paddingTop="5dip"
	    android:layout_width="fill_parent"
	    android:layout_height="wrap_content"/>
		<EditText android:id="@+id/CapabilityPollingPeriod"
	    android:layout_width="fill_parent"
	    android:layout_height="wrap_content"
	 	android:layout_marginLeft="5dip"
	 	android:layout_marginRight="5dip"
	 	android:maxLength="10" android:lines="1" android:inputType="numberSigned"/>

        <CheckBox android:id="@+id/SipKeepAlive"
		android:layout_height="wrap_content"
		android:layout_width="fill_parent"
		android:layout_marginLeft="5dip"
		android:layout_marginRight="5dip"
        android:text="@string/provisioning_enable_SIP_keep_alive"/>

        <CheckBox android:id="@+id/TelUriFormat"
		android:layout_height="wrap_content"
		android:layout_width="fill_parent"
		android:layout_marginLeft="5dip"
		android:layout_marginRight="5dip"
        android:text="@string/provisioning_Tel_URI_format"/>

		<CheckBox android:id="@+id/ImAlwaysOn"
		android:layout_height="wrap_content"
		android:layout_width="fill_parent"
		android:layout_marginLeft="5dip"
		android:layout_marginRight="5dip"
        android:text="@string/provisioning_IM_always_on"/>
		
		<CheckBox android:id="@+id/FtAlwaysOn"
		android:layout_height="wrap_content"
		android:layout_width="fill_parent"
		android:layout_marginLeft="5dip"
		android:layout_marginRight="5dip"
        android:text="@string/provisioning_FT_always_on"/>

        <CheckBox android:id="@+id/ImUseReports"
		android:layout_height="wrap_content"
		android:layout_width="fill_parent"
		android:layout_marginLeft="5dip"
		android:layout_marginRight="5dip"
        android:text="@string/provisioning_IM_delivery_reports"/>

        <CheckBox android:id="@+id/PermanentState"
		android:layout_height="wrap_content"
		android:layout_width="fill_parent"
		android:layout_marginLeft="5dip"
		android:layout_marginRight="5dip"
        android:text="@string/provisioning_permanent_state_mode"/>

        <CheckBox android:id="@+id/Gruu"
		android:layout_height="wrap_content"
		android:layout_width="fill_parent"
		android:layout_marginLeft="5dip"
		android:layout_marginRight="5dip"
        android:text="@string/provisioning_GRUU"/>

        <CheckBox android:id="@+id/CpuAlwaysOn"
		android:layout_height="wrap_content"
		android:layout_width="fill_parent"
		android:layout_marginLeft="5dip"
		android:layout_marginRight="5dip"
        android:text="@string/provisioning_CPU_Always_on"/>

        <CheckBox android:id="@+id/SecureMsrpOverWifi"
		android:layout_height="wrap_content"
		android:layout_width="fill_parent"
		android:layout_marginLeft="5dip"
		android:layout_marginRight="5dip"
        android:text="@string/provisioning_secure_MSRP_over_Wi_Fi_access"/>

        <CheckBox android:id="@+id/SecureRtpOverWifi"
		android:layout_height="wrap_content"
		android:layout_width="fill_parent"
		android:layout_marginLeft="5dip"
		android:layout_marginRight="5dip"
        android:text="@string/provisioning_secure_RTP_over_Wi_Fi_access"
        android:enabled="false"/>

        <CheckBox android:id="@+id/ImeiAsDeviceId"
		android:layout_height="wrap_content"
		android:layout_width="fill_parent"
		android:layout_marginLeft="5dip"
		android:layout_marginRight="5dip"
        android:text="@string/provisioning_IMEI_as_device_ID"/>
	</LinearLayout>
	</ScrollView>
</LinearLayout>
		