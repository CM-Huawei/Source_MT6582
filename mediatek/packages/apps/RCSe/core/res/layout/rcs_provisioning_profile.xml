<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent">
    
	<TableLayout android:layout_width="fill_parent"
	  android:layout_height="wrap_content"
	  android:layout_margin="5dip"
	  android:stretchColumns="0,1">
	    <TableRow android:layout_width="fill_parent"
	      android:layout_height="wrap_content">
		<Button android:text="@string/label_save"
		 android:layout_width="fill_parent"
		 android:layout_height="wrap_content"
		 android:id="@+id/save_btn"/>    
		<Button android:text="@string/label_generate_profile"
		 android:layout_width="fill_parent"
		 android:layout_height="wrap_content"
		 android:id="@+id/gen_btn"/>    
	    </TableRow>
	</TableLayout> 	    
	
	<ScrollView android:layout_width="fill_parent" android:layout_height="fill_parent">
	<LinearLayout
	    android:orientation="vertical"
	    android:layout_width="fill_parent"
	    android:layout_height="fill_parent">

       	<TextView android:text="@string/ims_authentication_procedure_mobile" 
	    android:paddingTop="5dip"
	    android:layout_width="fill_parent"
	    android:layout_height="wrap_content"/>
		<Spinner android:id="@+id/ImsAuhtenticationProcedureForMobile"
		android:layout_height="wrap_content"
		android:layout_width="fill_parent"
		android:layout_marginLeft="5dip"
		android:layout_marginRight="5dip"
		android:drawSelectorOnTop="true"/>

       	<TextView android:text="@string/ims_authentication_procedure_wifi"
	    android:paddingTop="5dip"
	    android:layout_width="fill_parent"
	    android:layout_height="wrap_content"/>
		<Spinner android:id="@+id/ImsAuhtenticationProcedureForWifi"
		android:layout_height="wrap_content"
		android:layout_width="fill_parent"
		android:layout_marginLeft="5dip"
		android:layout_marginRight="5dip"
		android:drawSelectorOnTop="true"/>

		<TextView android:text="@string/ims_username"
	    android:paddingTop="5dip"
	    android:layout_width="fill_parent"
	    android:layout_height="wrap_content"/>
		<EditText android:id="@+id/ImsUsername"
	    android:layout_width="fill_parent"
	    android:layout_height="wrap_content"
	 	android:layout_marginLeft="5dip"
	 	android:layout_marginRight="5dip"
	 	android:maxLength="100" android:lines="1"/>

       	<TextView android:text="@string/ims_display_name"
	    android:paddingTop="5dip"
	    android:layout_width="fill_parent"
	    android:layout_height="wrap_content"/>
		<EditText android:id="@+id/ImsDisplayName"
	    android:layout_width="fill_parent"
	    android:layout_height="wrap_content"
	 	android:layout_marginLeft="5dip"
	 	android:layout_marginRight="5dip"
	 	android:maxLength="100" android:lines="1"/>

       	<TextView android:text="@string/ims_home_domain"
	    android:paddingTop="5dip"
	    android:layout_width="fill_parent"
	    android:layout_height="wrap_content"/>
		<EditText android:id="@+id/ImsHomeDomain"
	    android:layout_width="fill_parent"
	    android:layout_height="wrap_content"
	 	android:layout_marginLeft="5dip"
	 	android:layout_marginRight="5dip"
	 	android:maxLength="100" android:lines="1"/>

       	<TextView android:text="@string/ims_private_uri"
	    android:paddingTop="5dip"
	    android:layout_width="fill_parent"
	    android:layout_height="wrap_content"/>
		<EditText android:id="@+id/ImsPrivateId"
	    android:layout_width="fill_parent"
	    android:layout_height="wrap_content"
	 	android:layout_marginLeft="5dip"
	 	android:layout_marginRight="5dip"
	 	android:maxLength="100" android:lines="1"/>

       	<TextView android:text="@string/ims_password"
	    android:paddingTop="5dip"
	    android:layout_width="fill_parent"
	    android:layout_height="wrap_content"/>
		<EditText android:id="@+id/ImsPassword"
	    android:layout_width="fill_parent"
	    android:layout_height="wrap_content"
	 	android:layout_marginLeft="5dip"
	 	android:layout_marginRight="5dip"
	 	android:maxLength="100" android:lines="1"/>

       	<TextView android:text="@string/ims_realm"
	    android:paddingTop="5dip"
	    android:layout_width="fill_parent"
	    android:layout_height="wrap_content"/>
		<EditText android:id="@+id/ImsRealm"
	    android:layout_width="fill_parent"
	    android:layout_height="wrap_content"
	 	android:layout_marginLeft="5dip"
	 	android:layout_marginRight="5dip"
	 	android:maxLength="100" android:lines="1"/>

       	<TextView android:text="@string/CISF_address_mobile"
	    android:paddingTop="5dip"
	    android:layout_width="fill_parent"
	    android:layout_height="wrap_content"/>
		<EditText android:id="@+id/ImsOutboundProxyAddrForMobile"
	    android:layout_width="fill_parent"
	    android:layout_height="wrap_content"
	 	android:layout_marginLeft="5dip"
	 	android:layout_marginRight="5dip"
	 	android:maxLength="100" android:lines="1"/>

       	<TextView android:text="@string/CISF_port_mobile"
	    android:paddingTop="5dip"
	    android:layout_width="fill_parent"
	    android:layout_height="wrap_content"/>
		<EditText android:id="@+id/ImsOutboundProxyPortForMobile"
	    android:layout_width="fill_parent"
	    android:layout_height="wrap_content"
	 	android:layout_marginLeft="5dip"
	 	android:layout_marginRight="5dip"
	 	android:maxLength="5" android:inputType="number" android:lines="1"/>

	 	<TextView android:text="@string/CISF_address_wifi"
	    android:paddingTop="5dip"
	    android:layout_width="fill_parent"
	    android:layout_height="wrap_content"/>
		<EditText android:id="@+id/ImsOutboundProxyAddrForWifi"
	    android:layout_width="fill_parent"
	    android:layout_height="wrap_content"
	 	android:layout_marginLeft="5dip"
	 	android:layout_marginRight="5dip"
	 	android:maxLength="100" android:lines="1"/>

	 	<TextView android:text="@string/CISF_port_wifi"
	    android:paddingTop="5dip"
	    android:layout_width="fill_parent"
	    android:layout_height="wrap_content"/>
		<EditText android:id="@+id/ImsOutboundProxyPortForWifi"
	    android:layout_width="fill_parent"
	    android:layout_height="wrap_content"
	 	android:layout_marginLeft="5dip"
	 	android:layout_marginRight="5dip"
	 	android:maxLength="5" android:inputType="number" android:lines="1"/>

	 	<TextView android:text="@string/XDM_server_address"
	    android:paddingTop="5dip"
	    android:layout_width="fill_parent"
	    android:layout_height="wrap_content"/>
		<EditText android:id="@+id/XdmServerAddr"
	    android:layout_width="fill_parent"
	    android:layout_height="wrap_content"
	 	android:layout_marginLeft="5dip"
	 	android:layout_marginRight="5dip"
	 	android:maxLength="100" android:lines="1"/>

       	<TextView android:text="@string/XDM_sever_login"
	    android:paddingTop="5dip"
	    android:layout_width="fill_parent"
	    android:layout_height="wrap_content"/>
		<EditText android:id="@+id/XdmServerLogin"
	    android:layout_width="fill_parent"
	    android:layout_height="wrap_content"
	 	android:layout_marginLeft="5dip"
	 	android:layout_marginRight="5dip"
	 	android:maxLength="100" android:lines="1"/>

       	<TextView android:text="@string/XDM_sever_passwd"
	    android:paddingTop="5dip"
	    android:layout_width="fill_parent"
	    android:layout_height="wrap_content"/>
		<EditText android:id="@+id/XdmServerPassword"
	    android:layout_width="fill_parent"
	    android:layout_height="wrap_content"
	 	android:layout_marginLeft="5dip"
	 	android:layout_marginRight="5dip"
	 	android:maxLength="100" android:lines="1"/>

		<TextView android:text="@string/FT_http_server_address"
	    android:paddingTop="5dip"
	    android:layout_width="fill_parent"
	    android:layout_height="wrap_content"/>
		<EditText android:id="@+id/FtHttpServerAddr"
	    android:layout_width="fill_parent"
	    android:layout_height="wrap_content"
	 	android:layout_marginLeft="5dip"
	 	android:layout_marginRight="5dip"
	 	android:maxLength="100" android:lines="1"/>

       	<TextView android:text="@string/FT_http_server_login"
	    android:paddingTop="5dip"
	    android:layout_width="fill_parent"
	    android:layout_height="wrap_content"/>
		<EditText android:id="@+id/FtHttpServerLogin"
	    android:layout_width="fill_parent"
	    android:layout_height="wrap_content"
	 	android:layout_marginLeft="5dip"
	 	android:layout_marginRight="5dip"
	 	android:maxLength="100" android:lines="1"/>

       	<TextView android:text="@string/FT_http_server_passwd"
	    android:paddingTop="5dip"
	    android:layout_width="fill_parent"
	    android:layout_height="wrap_content"/>
		<EditText android:id="@+id/FtHttpServerPassword"
	    android:layout_width="fill_parent"
	    android:layout_height="wrap_content"
	 	android:layout_marginLeft="5dip"
	 	android:layout_marginRight="5dip"
	 	android:maxLength="100" android:lines="1"/>
		
       	<TextView android:text="@string/im_conference_ui"
	    android:paddingTop="5dip"
	    android:layout_width="fill_parent"
	    android:layout_height="wrap_content"/>
		<EditText android:id="@+id/ImConferenceUri"
	    android:layout_width="fill_parent"
	    android:layout_height="wrap_content"
	 	android:layout_marginLeft="5dip"
	 	android:layout_marginRight="5dip"
	 	android:maxLength="100" android:lines="1"/>

       	<TextView android:text="@string/end_user_uri"
	    android:paddingTop="5dip"
	    android:layout_width="fill_parent"
	    android:layout_height="wrap_content"/>
		<EditText android:id="@+id/EndUserConfReqUri"
	    android:layout_width="fill_parent"
	    android:layout_height="wrap_content"
	 	android:layout_marginLeft="5dip"
	 	android:layout_marginRight="5dip"
	 	android:maxLength="100" android:lines="1"/>

       	<TextView android:text="@string/provisioning_apn"
	    android:paddingTop="5dip"
	    android:layout_width="fill_parent"
	    android:layout_height="wrap_content"/>
		<EditText android:id="@+id/RcsApn"
	    android:layout_width="fill_parent"
	    android:layout_height="wrap_content"
	 	android:layout_marginLeft="5dip"
	 	android:layout_marginRight="5dip"
	 	android:lines="1"/>

       	<TextView android:text="@string/provisioning_country_code"
	    android:paddingTop="5dip"
	    android:layout_width="fill_parent"
	    android:layout_height="wrap_content"/>
		<EditText android:id="@+id/CountryCode"
	    android:layout_width="fill_parent"
	    android:layout_height="wrap_content"
	 	android:layout_marginLeft="5dip"
	 	android:layout_marginRight="5dip"
	 	android:maxLength="4"
	 	android:lines="1"/>

       	<TextView android:text="@string/provisioning_country_area_code"
	    android:paddingTop="5dip"
	    android:layout_width="fill_parent"
	    android:layout_height="wrap_content"/>
		<EditText android:id="@+id/CountryAreaCode"
	    android:layout_width="fill_parent"
	    android:layout_height="wrap_content"
	 	android:layout_marginLeft="5dip"
	 	android:layout_marginRight="5dip"
	 	android:maxLength="4"
	 	android:lines="1"/>

	 	<TextView android:text="@string/provisioning_capabilities"
	    android:paddingTop="5dip"
	    android:layout_width="fill_parent"
	    android:layout_height="wrap_content"/>

		<CheckBox android:id="@+id/image_sharing"
	    android:layout_width="fill_parent"
	    android:layout_height="wrap_content"
        android:textStyle="bold"
		android:enabled="true"
		android:text="@string/provisioning_image_sharinge" />

        <CheckBox android:id="@+id/video_sharing"
	    android:layout_width="fill_parent"
	    android:layout_height="wrap_content"
        android:textStyle="bold"
		android:enabled="true"
        android:text="@string/provisioning_video_sharinge" />

        <CheckBox android:id="@+id/file_transfer"
	    android:layout_width="fill_parent"
	    android:layout_height="wrap_content"
        android:textStyle="bold"
		android:enabled="true"
        android:text="@string/provisioning_file_transfer" />

        <CheckBox android:id="@+id/file_transfer_http"
	    android:layout_width="fill_parent"
	    android:layout_height="wrap_content"
        android:textStyle="bold"
		android:enabled="true"
        android:text="@string/provisioning_file_transfer_http" />
        
        <CheckBox android:id="@+id/im"
	    android:layout_width="fill_parent"
	    android:layout_height="wrap_content"
        android:textStyle="bold"
        android:text="@string/provisioning_im_session" />
        
        <CheckBox android:id="@+id/im_group"
	    android:layout_width="fill_parent"
	    android:layout_height="wrap_content"
        android:textStyle="bold"
        android:text="@string/provisioning_group_sesion" />
        
        <CheckBox android:id="@+id/ipvoicecall"
	    android:layout_width="fill_parent"
	    android:layout_height="wrap_content"
        android:textStyle="bold"
        android:enabled="true"
        android:text="@string/provisioning_ip_voice_call" />
        
        <CheckBox android:id="@+id/ipvideocall"
	    android:layout_width="fill_parent"
	    android:layout_height="wrap_content"
        android:textStyle="bold"
        android:enabled="true"
        android:text="@string/provisioning_ip_video_call" />

        <CheckBox android:id="@+id/cs_video"
	    android:layout_width="fill_parent"
	    android:layout_height="wrap_content"
        android:textStyle="bold"
		android:enabled="true"
        android:text="@string/provisioning_cs_video" />

        <CheckBox android:id="@+id/presence_discovery"
	    android:layout_width="fill_parent"
	    android:layout_height="wrap_content"
        android:textStyle="bold"
		android:enabled="true"
        android:text="@string/provisioning_presence_dicovery" />

		<CheckBox android:id="@+id/social_presence"
	    android:layout_width="fill_parent"
	    android:layout_height="wrap_content"
	    android:textStyle="bold"
		android:enabled="true"
        android:text="@string/provisioning_social_presence" />
		
		<CheckBox android:id="@+id/geolocation_push"
	    android:layout_width="fill_parent"
	    android:layout_height="wrap_content"
	    android:textStyle="bold"
		android:enabled="true"
        android:text="@string/provisioning_geolocation_push" />
		
		<CheckBox android:id="@+id/file_transfer_thumbnail"
	    android:layout_width="fill_parent"
	    android:layout_height="wrap_content"
        android:textStyle="bold"
		android:enabled="true"
        android:text="@string/provisioning_ft_thumbnail" />

		<CheckBox android:id="@+id/file_transfer_sf"
	    android:layout_width="fill_parent"
	    android:layout_height="wrap_content"
        android:textStyle="bold"
		android:enabled="true"
        android:text="@string/provisioning_ft_store_and_forward" />

		<CheckBox android:id="@+id/group_chat_sf"
	    android:layout_width="fill_parent"
	    android:layout_height="wrap_content"
        android:textStyle="bold"
		android:enabled="true"
        android:text="@string/provisioning_group_chat_store_and_forward" />
	
<CheckBox android:id="@+id/sip_automata"
	    android:layout_width="fill_parent"
	    android:layout_height="wrap_content"
        android:textStyle="bold"
		android:enabled="true"
        android:text="@string/provisioning_sip_automata" />
	
		<TextView
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:paddingTop="5dip"
        android:text="@string/provisioning_gsma_release" />

        <Spinner
        android:id="@+id/GsmaRelease"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="5dip"
        android:layout_marginRight="5dip"
        android:drawSelectorOnTop="true" />

	</LinearLayout>
	</ScrollView>
	
</LinearLayout>
    