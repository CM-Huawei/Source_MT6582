<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    >

    
<TextView 
	android:layout_width="fill_parent" 
	android:layout_height="wrap_content"
	android:id="@+id/accuracy_label" 
   	android:text="@string/accuracy_unknown"
   	android:textSize="20sp"
    />
    
<TextView android:layout_width="fill_parent" android:layout_height="wrap_content" android:id="@+id/yaw_raw" android:textSize="12sp"/>
<TextView android:layout_width="fill_parent" android:layout_height="wrap_content" android:id="@+id/yaw_cal" android:textSize="12sp"/>
<TextView android:layout_width="fill_parent" android:layout_height="wrap_content" android:id="@+id/yaw_max" android:textSize="12sp"/>
<TextView android:layout_width="fill_parent" android:layout_height="wrap_content" android:id="@+id/yaw_min" android:textSize="12sp"/>

<TextView android:layout_width="fill_parent" android:layout_height="wrap_content" android:id="@+id/pit_raw" android:textSize="12sp"/>
<TextView android:layout_width="fill_parent" android:layout_height="wrap_content" android:id="@+id/pit_cal" android:textSize="12sp"/>
<TextView android:layout_width="fill_parent" android:layout_height="wrap_content" android:id="@+id/pit_max" android:textSize="12sp"/>
<TextView android:layout_width="fill_parent" android:layout_height="wrap_content" android:id="@+id/pit_min" android:textSize="12sp"/>

<TextView android:layout_width="fill_parent" android:layout_height="wrap_content" android:id="@+id/rol_raw" android:textSize="12sp"/>
<TextView android:layout_width="fill_parent" android:layout_height="wrap_content" android:id="@+id/rol_cal" android:textSize="12sp"/>
<TextView android:layout_width="fill_parent" android:layout_height="wrap_content" android:id="@+id/rol_max" android:textSize="12sp"/>
<TextView android:layout_width="fill_parent" android:layout_height="wrap_content" android:id="@+id/rol_min" android:textSize="12sp"/>

