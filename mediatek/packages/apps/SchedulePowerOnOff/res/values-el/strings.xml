            <xliff:g id="MINUTES" example="2minutes">%3$s</xliff:g> από τώρα.
        </item> 
    </string-array>  
    <!-- Alarm confirmation toast: singular day -->  
    <string name="day">"1 ημέρα"</string>  
    <!-- Alarm confirmation toast: plural days -->  
    <string name="days">" 
        <xliff:g id="days">%s</xliff:g> ημέρες"
    </string>  
    <!-- Alarm confirmation toast: singular hour -->  
    <string name="hour">"1 ώρα"</string>  
    <!-- Alarm confirmation toast: plural hours -->  
    <string name="hours"> 
        <xliff:g id="hours">%s</xliff:g> ώρες
    </string>  
    <!-- Alarm confirmation toast: singular minute  -->  
    <string name="minute">"1 λεπτό"</string>  
    <!-- Alarm confirmation toast: plural minutes  -->  
    <string name="minutes">" 
        <xliff:g id="minutes">%s</xliff:g> λεπτά"
    </string>  
    <!-- Repeat options that appear under an alarm on main Alarm Clock
         screen to identify repetition schedule: special case for when
         the alarm is set to repeat every day -->  
    <string name="every_day">κάθε μέρα</string>  
    <!-- Repeat options that appear under an alarm on main Alarm Clock
         screen to identify repetition schedule: special case for when
         the alarm is set to never repeat -->  
    <string name="never">Ποτέ</string>  
    <!-- Done button when editing an alarm. -->  
    <string name="done">Τέλος</string>  
    <!-- Revert button when editing an alarm. -->  
    <string name="revert">"Επαναφορά"</string>  
    <string name="schpwr_shutdown_message">"Η συσκευή σας θα απενεργοποιηθεί σε 
        <xliff:g id="seconds">%s</xliff:g> δευτερόλεπτα"
    </string>  
    <string name="schpwr_shutdown_message_second">"Η συσκευή σας θα απενεργοποιηθεί σε 
