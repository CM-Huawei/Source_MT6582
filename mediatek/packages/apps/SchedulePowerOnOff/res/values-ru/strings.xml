    <string name="done">Готово</string>  
    <!-- Revert button when editing an alarm. -->  
    <string name="revert">"Отмена"</string>  
    <string name="schpwr_shutdown_message">"Телефон выключится через 
        <xliff:g id="seconds">%s</xliff:g> секунд(ы)"
    </string>  
    <string name="schpwr_shutdown_message_second">"Телефон выключится через 
        <xliff:g id="second">%s</xliff:g> секунду"
