    <string name="day">"1 день"</string>  
    <!-- Alarm confirmation toast: plural days -->  
    <string name="days">" 
        <xliff:g id="days">%s</xliff:g> дн."
    </string>  
    <!-- Alarm confirmation toast: singular hour -->  
    <string name="hour">"за 1 час"</string>  
    <!-- Alarm confirmation toast: plural hours -->  
    <string name="hours"> 
        <xliff:g id="hours">%s</xliff:g> час.
    </string>  
    <!-- Alarm confirmation toast: singular minute  -->  
    <string name="minute">"1 мин."</string>  
    <!-- Alarm confirmation toast: plural minutes  -->  
    <string name="minutes">" 
        <xliff:g id="minutes">%s</xliff:g> мин."
    </string>  
