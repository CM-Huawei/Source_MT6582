      <!-- Schedule power on/off settings -->  
    <!-- Main Settings screen setting option name to go into the power on/off settings-->  
    <string name="schedule_power_on_off_settings_title">Lịch bật/tắt nguồn</string>  
    <!-- Main Settings screen setting option summary text for the item to go into the date and time settings. -->  
    <string name="schedule_power_on_off_time">Giờ</string>  
    <!-- Date & time setting screen setting check box title if the date and time should be determined automatically -->  
    <string name="schedule_power_on_off_repeat">Lặp lại</string>  
    <!-- Date & time setting screen setting option summary text when Automatic check box is selected (that is, when date and time should be determined automatically) -->  
    <string name="schedule_power_on_off_set">Cài lịch bật/tắt nguồn</string>  
    <string name="schedule_power_on_set">Cài lịch bật nguồn</string>  
    <string name="schedule_power_off_set">"Cài lịch tắt nguồn"</string>  
    <!-- Context Menu Item on Alarm Settings screen: Enable alarm -->  
    <string name="enable_schpwr">Bật</string>  
    <!-- Context Menu Item on Alarm Settings screen: Disable alarm -->  
    <string name="disable_schpwr">Tắt</string>  
    <!-- Alarm confirmation toast: Describes how long from now until
         alarm fires -->  
    <string-array name="alarm_set"> 
        <item>Bật nguồn này được cài trong dưới 1 phút tính từ bây giờ.</item>  
        <item>Bật nguồn này được cài trong 
            <xliff:g example="2days" id="DAYS">%1$s</xliff:g> tính từ giờ.
        </item>  
        <item>Đã cài bật nguồn này cho 
            <xliff:g id="HOURS" example="2hours">%2$s</xliff:g> từ bây giờ.
        </item>  
        <item>Bật nguồn này được cài trong 
            <xliff:g id="DAYS" example="2days">%1$s</xliff:g> và 
            <xliff:g id="HOURS" example="2hours">%2$s</xliff:g> tính từ giờ.
        </item>  
        <item>Đã cài bật nguồn này cho 
            <xliff:g id="MINUTES" example="2minutes">%3$s</xliff:g> từ bây giờ.
        </item>  
        <item>Bật nguồn này được cài trong 
            <xliff:g id="DAYS" example="2days">%1$s</xliff:g> và 
            <xliff:g id="MINUTES" example="2minutes">%3$s</xliff:g> tính từ giờ.
        </item>  
        <item>Đã cài bật nguồn này cho 
            <xliff:g id="HOURS" example="2hours">%2$s</xliff:g> và 
            <xliff:g id="MINUTES" example="2minutes">%3$s</xliff:g> từ bây giờ.
        </item>  
        <item>Bật nguồn này được cài trong 
            <xliff:g id="DAYS" example="2days">%1$s</xliff:g>, 
            <xliff:g id="HOURS" example="2hours">%2$s</xliff:g> và 
            <xliff:g id="MINUTES" example="2minutes">%3$s</xliff:g> tính từ giờ.
        </item>  
        <item>Tắt nguồn này được cài trong dưới 1 phút tính từ bây giờ.</item>  
        <item>Tắt nguồn này được cài trong 
            <xliff:g id="DAYS" example="2days">%1$s</xliff:g> tính từ giờ.
        </item>  
        <item>Đã cài tắt nguồn này cho 
            <xliff:g id="HOURS" example="2hours">%2$s</xliff:g> từ bây giờ.
        </item>  
        <item>Tắt nguồn này được cài trong 
            <xliff:g id="DAYS" example="2days">%1$s</xliff:g> và 
            <xliff:g id="HOURS" example="2hours">%2$s</xliff:g> tính từ giờ.
        </item>  
        <item>Đã cài tắt nguồn này cho 
            <xliff:g id="MINUTES" example="2minutes">%3$s</xliff:g> từ bây giờ.
        </item>  
        <item>Tắt nguồn này được cài trong 
            <xliff:g id="DAYS" example="2days">%1$s</xliff:g> và 
            <xliff:g id="MINUTES" example="2minutes">%3$s</xliff:g> tính từ giờ.
        </item>  
        <item>Đã cài tắt nguồn này cho 
            <xliff:g id="HOURS" example="2hours">%2$s</xliff:g> và 
            <xliff:g id="MINUTES" example="2minutes">%3$s</xliff:g> từ bây giờ.
        </item>  
        <item>Tắt nguồn này được cài trong 
            <xliff:g id="DAYS" example="2days">%1$s</xliff:g>, 
            <xliff:g id="HOURS" example="2hours">%2$s</xliff:g> và 
            <xliff:g id="MINUTES" example="2minutes">%3$s</xliff:g> tính từ giờ.
        </item> 
    </string-array>  
    <!-- Alarm confirmation toast: singular day -->  
    <string name="day">"1 ngày"</string>  
    <!-- Alarm confirmation toast: plural days -->  
    <string name="days">" 
        <xliff:g id="days">%s</xliff:g> ngày"
    </string>  
    <!-- Alarm confirmation toast: singular hour -->  
    <string name="hour">1 giờ</string>  
    <!-- Alarm confirmation toast: plural hours -->  
    <string name="hours"> 
        <xliff:g id="hours">%s</xliff:g> tiếng
    </string>  
    <!-- Alarm confirmation toast: singular minute  -->  
    <string name="minute">"1 phút"</string>  
    <!-- Alarm confirmation toast: plural minutes  -->  
    <string name="minutes">" 
        <xliff:g id="minutes">%s</xliff:g> phút"
    </string>  
    <!-- Repeat options that appear under an alarm on main Alarm Clock
         screen to identify repetition schedule: special case for when
         the alarm is set to repeat every day -->  
    <string name="every_day">hàng ngày</string>  
    <!-- Repeat options that appear under an alarm on main Alarm Clock
         screen to identify repetition schedule: special case for when
         the alarm is set to never repeat -->  
    <string name="never">K.bao giờ</string>  
    <!-- Repeat options that appear under an alarm on main Alarm Clock
         screen to identify repetition schedule: concatenate days with
         this character, i.e. "Mon, Tue, Wed" -->  
    <!-- Done button when editing an alarm. -->  
    <string name="done">Hoàn tất</string>  
    <!-- Revert button when editing an alarm. -->  
    <string name="revert">"Hoàn nguyên"</string>  
    <string name="schpwr_shutdown_message">"ĐT của bạn sẽ tắt trong 
        <xliff:g id="seconds">%s</xliff:g> giây"
    </string>  
    <string name="schpwr_shutdown_message_second">"ĐT của bạn sẽ tắt trong 
        <xliff:g id="seconds">%s</xliff:g> giây"
    </string>  
</resources>
