    <!-- Alarm confirmation toast: singular day -->  
    <string name="day">"1 zi"</string>  
    <!-- Alarm confirmation toast: plural days -->  
    <string name="days">" 
        <xliff:g id="days">%s</xliff:g> zile"
    </string>  
    <!-- Alarm confirmation toast: singular hour -->  
    <string name="hour">"1 oră"</string>  
    <!-- Alarm confirmation toast: plural hours -->  
    <string name="hours"> 
        <xliff:g id="hours">%s</xliff:g> ore
    </string>  
    <!-- Alarm confirmation toast: singular minute  -->  
    <string name="minute">"1 minut"</string>  
    <!-- Alarm confirmation toast: plural minutes  -->  
    <string name="minutes">" 
        <xliff:g id="minutes">%s</xliff:g> minute"
    </string>  
    <!-- Repeat options that appear under an alarm on main Alarm Clock
         screen to identify repetition schedule: special case for when
         the alarm is set to repeat every day -->  
    <string name="every_day">fiecare zi</string>  
    <!-- Repeat options that appear under an alarm on main Alarm Clock
         screen to identify repetition schedule: special case for when
         the alarm is set to never repeat -->  
    <string name="never">Niciodată</string>  
    <!-- Repeat options that appear under an alarm on main Alarm Clock
         screen to identify repetition schedule: concatenate days with
         this character, i.e. "Mon, Tue, Wed" -->  
    <!-- Done button when editing an alarm. -->  
    <string name="done">Finalizat</string>  
    <!-- Revert button when editing an alarm. -->  
    <string name="revert">"Reveniţi"</string>  
    <string name="schpwr_shutdown_message">"Dispozitivul se va închide în 
        <xliff:g id="seconds">%s</xliff:g> secunde"
    </string>  
    <string name="schpwr_shutdown_message_second">"Dispozitivul se va închide în 
        <xliff:g id="second">%s</xliff:g> secundă"
    </string>  
