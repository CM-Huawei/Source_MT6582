            <intent-filter>
                <!-- action android:name="android.bluetooth.adapter.action.STATE_CHANGED"/ -->
        		<action android:name="android.provider.Telephony.SECRET_CODE" />
        		<data android:scheme="android_secret_code" android:host="2872710" />
            </intent-filter>
        </receiver>

	</application>

</manifest> 
