<!-- These resources are around just to allow their values to be customized
     for different hardware and product builds. -->
<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">
    
    <!-- List of regexpressions describing the interface (if any) that represent tetherable
         bluetooth interfaces.  If the device doesn't want to support tething over bluetooth this should
         be empty.  -->
    <string-array translatable="false" name="config_tether_bluetooth_regexs">
        <item>"bt-pan"</item>    
        <item>"bt-dun"</item>     
    </string-array>

    <!-- Dhcp range (min, max) to use for bluetooth tethering purposes -->
    <string-array translatable="false" name="config_bluetooth_tether_dhcp_range">
    	<item>"192.168.5.2"</item>
    	<item>"192.168.5.254"</item>
    	<item>"192.168.6.2"</item>
    	<item>"192.168.6.254"</item>
    </string-array>
    
    <!-- DNS server to use for bluetooth tethering purposes -->
    <string-array translatable="false" name="config_bluetooth_tether_dns_server">
    	<item>"8.8.8.8"</item>
    	<item>"4.2.2.2"</item>
    </string-array>

    <!-- Regex array of allowable upstream ifaces for bluetooth tethering  -->
    <string-array translatable="false" name="config_tether_bluetooth_upstream_regexs">
    	<item>"wlan\\d"</item>
	<item>"ccmni\\d"</item>
    </string-array>
    
</resources>
