			<CheckBox android:text="@string/NFC_SoftwareStack_DN_ISO14443B" android:id="@+id/NFC_SoftwareStack_DN_ISO14443B"
			android:layout_width="fill_parent" android:layout_height="wrap_content"></CheckBox>			
