<!-- M: Support "SystemUI - Dual SIM". @{ -->
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/carrier_label_gemini"
    android:layout_width="match_parent"
    android:layout_height="@dimen/carrier_label_height"
    android:layout_gravity="bottom"
    android:layout_marginBottom="@dimen/close_handle_height"
    android:gravity="center"
    android:orientation="horizontal"
    android:visibility="invisible">
    <com.mediatek.systemui.statusbar.util.CarrierLabel
        android:id="@+id/carrier1"
        android:textAppearance="@style/TextAppearance.StatusBar.Expanded.Network"
        android:layout_width="0dip"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        android:layout_gravity="center"
        android:gravity="center"
        android:paddingLeft="12dp"
        android:paddingRight="12dp"
        android:singleLine="true"
        />
    <ImageView
        android:id="@+id/carrier_divider"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:src="@drawable/gemini_carrier_divider"
    />
    <com.mediatek.systemui.statusbar.util.CarrierLabel
        android:id="@+id/carrier2"
        android:textAppearance="@style/TextAppearance.StatusBar.Expanded.Network"
        android:layout_width="0dip"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        android:layout_gravity="center"
        android:gravity="center"
        android:paddingLeft="12dp"
        android:paddingRight="12dp"
        android:singleLine="true"
        />
    <ImageView
        android:id="@+id/carrier_divider2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:src="@drawable/gemini_carrier_divider"
        android:visibility="gone"
    />
    <com.mediatek.systemui.statusbar.util.CarrierLabel
        android:id="@+id/carrier3"
        android:textAppearance="@style/TextAppearance.StatusBar.Expanded.Network"
        android:layout_width="0dip"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        android:layout_gravity="center"
        android:gravity="center"
        android:paddingLeft="12dp"
        android:paddingRight="12dp"
        android:singleLine="true"
        android:visibility="gone"
    />
    <ImageView
        android:id="@+id/carrier_divider3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:src="@drawable/gemini_carrier_divider"
        android:visibility="gone"
    />
    <com.mediatek.systemui.statusbar.util.CarrierLabel
        android:id="@+id/carrier4"
        android:textAppearance="@style/TextAppearance.StatusBar.Expanded.Network"
        android:layout_width="0dip"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        android:layout_gravity="center"
        android:gravity="center"
        android:paddingLeft="12dp"
        android:paddingRight="12dp"
        android:singleLine="true"
        android:visibility="gone"
    />
</LinearLayout>
<!-- M: Support "SystemUI - Dual SIM". }@ -->